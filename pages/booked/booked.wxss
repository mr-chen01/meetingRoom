/* pages/booked/booked.wxss */
page{
  background-color:#F5F5F5;
}

.container{
  width:750rpx;
  height:1185rpx;
  padding-top:50rpx;
}

.single{
  width: 700rpx;
  height:200rpx;
  padding-top:50rpx;
  margin-bottom: 50rpx;
  display: flex;
  flex-direction: row;
  background-color: white;
  border-radius: 20rpx;
}

.single:active{
  background-color: #DCDCDC;
}

.time{
  width: 200rpx;
}

.single view{
  display: flex;
  flex-direction: column;
  align-items:center;
  justify-content: center;
}

.single text{
  margin-bottom: 50rpx;
  text-align: center;
}

.body{
  width:500rpx;
}

button{
  position: fixed;
  left:250rpx;
  bottom:100rpx;
  background-color: #7FFFAA;
  opacity: 0.7;
}

button :active{
  background-color:#32CD32;
}